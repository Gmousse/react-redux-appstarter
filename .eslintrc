{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "classes": true,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [2, 4],
        "no-console": 0,
        "react/jsx-closing-bracket-location": 0,
        "react/prop-types": 0,
        "object-curly-spacing": 0
    }
}
